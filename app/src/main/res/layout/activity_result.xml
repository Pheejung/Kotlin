<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ResultActivity">
    <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline1"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.3"/>
    <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline2"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.5"/>
    <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline3"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.08"/>
    <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline4"
            app:layout_constraintGuide_begin="20dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.92"/>
    <view
            android:layout_width="0dp"
            android:layout_height="0dp"
            class="android.support.v7.widget.AppCompatTextView"
            id="@+id/view"
            android:id="@+id/resultLabel"
            app:layout_constraintEnd_toStartOf="@+id/guideline4"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            app:layout_constraintStart_toStartOf="@+id/guideline3"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline1"
            android:textSize="60sp"
            android:text="홍길동님의\n로또번호"
            android:gravity="center"
            android:maxLines="3"
            app:autoSizeMaxTextSize="60sp"
            app:autoSizeMinTextSize="16sp"
            app:autoSizeStepGranularity="1sp"
            app:autoSizeTextType="uniform"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/imageView3"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline2"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/guideline4"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintStart_toStartOf="@+id/guideline3"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="82dp"
            android:layout_height="40dp"
            android:gravity="center"
            android:id="@+id/textView01"
            app:layout_constraintStart_toStartOf="@+id/guideline3"
            android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toTopOf="@+id/guideline1"
            android:textSize="20sp"/>
    <TextView
            android:gravity="center"
            android:text="TextView"
            android:layout_width="82dp"
            android:layout_height="40dp"
            android:id="@+id/textView02"
            app:layout_constraintStart_toEndOf="@+id/textView01"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toTopOf="@+id/guideline1"
            android:textSize="20sp"/>
    <TextView
            android:gravity="center"
            android:text="TextView"
            android:layout_width="82dp"
            android:layout_height="40dp"
            android:id="@+id/textView06"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView03"
            app:layout_constraintStart_toEndOf="@+id/textView05"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:textSize="20sp"/>
    <TextView
            android:gravity="center"
            android:text="TextView"
            android:layout_width="82dp"
            android:layout_height="40dp"
            android:id="@+id/textView04"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView01"
            app:layout_constraintStart_toStartOf="@+id/guideline3"
            android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"
            android:textSize="20sp"/>
    <TextView
            android:gravity="center"
            android:text="TextView"
            android:layout_width="82dp"
            android:layout_height="40dp"
            android:id="@+id/textView03"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toTopOf="@+id/guideline1"
            app:layout_constraintStart_toEndOf="@+id/textView02"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:textSize="20sp"/>
    <TextView
            android:gravity="center"
            android:text="TextView"
            android:layout_width="82dp"
            android:layout_height="40dp"
            android:id="@+id/textView05"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView02"
            app:layout_constraintStart_toEndOf="@+id/textView04"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:textSize="20sp"/>
</android.support.constraint.ConstraintLayout>



